{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "name": "jewelia",
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/",
      "NPM_CONFIG_FETCH_TIMEOUT": "300000",
      "NPM_CONFIG_FETCH_RETRIES": "5"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"]
} 